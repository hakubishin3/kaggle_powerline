
{
    "features": [
        "Meta",
        "Basic"
    ],
    "cv": {
        "method": "StratifiedKFold",
        "n_splits": 5,
        "random_state": 71
    },
    "model": {
        "name": "lightgbm",
        "model_params": {
            "boosting_type": "gbdt",
            "objective": "binary",
            "metric": "auc",
            "learning_rate": 0.05,
            "num_leaves": 100,
            "max_depth": -1,
            "subsample": 0.7,
            "subsample_freq": 1,
            "colsample_bytree": 0.5,
            "reg_alpha": 0.0,
            "reg_lambda": 0.0,
            "min_split_gain": 0.0,
            "seed": 71,
            "bagging_seed": 71,
            "feature_fraction_seed": 71,
            "drop_seed": 71,
            "random_state": 71,
            "verbosity": -1
        },
        "train_params": {
            "num_boost_round": 1000,
            "early_stopping_rounds": 100,
            "verbose_eval": 50
        }
    },
    "dataset": {
        "input_directory": "data/input/",
        "intermediate_directory": "data/interim/",
        "feature_directory": "data/feature/",
        "output_directory": "data/output/",
        "files": {
            "meta":{
                "train": "metadata_train.csv",
                "test": "metadata_test.csv"
            },
            "ts":{
                "train": "train.parquet",
                "test": "test.parquet"
            }
        }
    }
}